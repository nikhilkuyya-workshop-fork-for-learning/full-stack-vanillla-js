{
  "configurations": [
    {
      "name":"Run Test Debugger",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "test:dev"
      ],
      "cwd": "${fileDirname}",
      "request": "launch",
      "skipFiles": [
        "node_modules/**",
        "<node_internals>/**/**"
      ],
      "console": "integratedTerminal"
    },
    {
      "name":"Run CLI ",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "cli:dev"
      ],
      "cwd": "${fileDirname}",
      "request": "launch",
      "skipFiles": [
        "node_modules/**",
        "<node_internals>/**/**"
      ],
      "console": "integratedTerminal"
    },
    ]
}
